{
  "Agent Template": {
    "prefix": "agent",
    "body": [
      "<Agentinstructions>",
      "\t<Role>",
      "\t\t<Name>$1</Name>",
      "\t\t<Description>$2</Description>",
      "\t\t<currentDate>{{$now}}</currentDate>",
      "\t</Role>",
      "\t<Goal>",
      "\t\t<Primary>$3</Primary>",
      "\t</Goal>",
      "\t<Instructions>",
      "\t\t<Instruction>$4</Instruction>",
      "\t</Instructions>",
      "</Agentinstructions>"
    ],
    "description": "Базовый шаблон агента с секциями Role, Goal и Instructions"
  },
  "Full Agent Template": {
    "prefix": "agentfull",
    "body": [
      "<Agentinstructions>",
      "\t<Role>",
      "\t\t<Name>$1</Name>",
      "\t\t<Description>$2</Description>",
      "\t\t<currentDate>{{$now}}</currentDate>",
      "\t</Role>",
      "\t<Goal>",
      "\t\t<Primary>$3</Primary>",
      "\t\t<Secondary>$4</Secondary>",
      "\t</Goal>",
      "\t<Instructions>",
      "\t\t<Instruction>$5</Instruction>",
      "\t\t<Instruction>$6</Instruction>",
      "\t\t<Instruction>$7</Instruction>",
      "\t\t<Instruction>$8</Instruction>",
      "\t</Instructions>",
      "\t<Tools>",
      "\t\t<Tool>$9</Tool>",
      "\t</Tools>",
      "\t<Constraints>",
      "\t\t<Constraint>$10</Constraint>",
      "\t</Constraints>",
      "</Agentinstructions>"
    ],
    "description": "Полный шаблон агента со всеми возможными секциями"
  },
  "XML Declaration": {
    "prefix": "xml",
    "body": [
      "<?xml version=\"1.0\" encoding=\"UTF-8\"?>"
    ],
    "description": "XML декларация (если нужна)"
  },
  "Agentinstructions Root": {
    "prefix": "agentinstructions",
    "body": [
      "<Agentinstructions>",
      "\t$1",
      "</Agentinstructions>"
    ],
    "description": "Корневой элемент для инструкций агента"
  },
  "Role Section": {
    "prefix": "role",
    "body": [
      "<Role>",
      "\t<Name>$1</Name>",
      "\t<Description>$2</Description>",
      "\t<currentDate>{{$now}}</currentDate>",
      "\t$3",
      "</Role>"
    ],
    "description": "Секция роли агента"
  },
  "Name Section": {
    "prefix": "name",
    "body": [
      "<Name>$1</Name>"
    ],
    "description": "Имя агента"
  },
  "Description Section": {
    "prefix": "description",
    "body": [
      "<Description>$1</Description>"
    ],
    "description": "Описание агента"
  },
  "Current Date": {
    "prefix": "currentdate",
    "body": [
      "<currentDate></currentDate>"
    ],
    "description": "Текущая дата с переменной n8n"
  },
  "Goal Section": {
    "prefix": "goal",
    "body": [
      "<Goal>",
      "\t<Primary>$1</Primary>",
      "\t$2",
      "</Goal>"
    ],
    "description": "Секция целей агента"
  },
  "Primary Goal": {
    "prefix": "primary",
    "body": [
      "<Primary>$1</Primary>"
    ],
    "description": "Основная цель агента"
  },
  "Secondary Goal": {
